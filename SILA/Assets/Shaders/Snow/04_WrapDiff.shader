			#else //aselc
			float4 ase_lightColor = _LightColor0;
			#endif //aselc
			float4 temp_output_9_0 = ( ( saturate( ( ( 1.0 + dotResult4 ) / 2.0 ) ) * ase_lightAtten ) * ase_lightColor );
			c.rgb = temp_output_9_0.rgb;
			c.a = 1;
			return c;
		}

